{
  "name": "@ean-studio/backend-template",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@9.14.2",
  "main": "dist/src/main",
  "scripts": {
    "postinstall": "pnpm prisma generate",
    "prepare": "ts-patch install",
    "--------BUILD--------": "",
    "build": "nest build",
    "build:sdk": "pnpm mkdir sdk && npx nestia sdk",
    "--------UTILITY--------": "",
    "mkdir": "node scripts/mkdir.js",
    "--------LINTER--------": "",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "--------PRETTIER--------": "",
    "format": "prettier --write \"src/**/*.ts\"",
    "--------RUN--------": "",
    "dev": "nest start --watch",
    "prod": "node .",
    "--------TEST--------": "",
    "test:generate": "pnpm mkdir sdk && pnpm nestia e2e && prettier --write \"test/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run test/e2e/**/*.ts",
    "test:e2e:watch": "vitest --watch test/e2e/**/*.ts",
    "test:api": "vitest run test/features/api/**/*.ts",
    "test:api:watch": "vitest --watch test/features/api/**/*.ts",
    "test:api:coverage": "vitest run --coverage test/features/api/**/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/static": "^8.1.1",
    "@fastify/swagger": "^9.4.2",
    "@nestia/core": "^5.0.0",
    "@nestia/e2e": "^0.8.3",
    "@nestia/fetcher": "^5.0.0",
    "@nestia/sdk": "^5.0.0",
    "@nestjs-cls/transactional": "^2.6.1",
    "@nestjs-cls/transactional-adapter-prisma": "^1.2.19",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/platform-fastify": "^11.0.12",
    "@nestjs/swagger": "^11.1.0",
    "@prisma/client": "^6.5.0",
    "@prisma/extension-read-replicas": "^0.4.1",
    "@types/passport-google-oauth20": "^2.0.16",
    "cache-manager": "^6.4.1",
    "cache-manager-redis-store": "^3.0.1",
    "fastify-raw-body": "^5.0.0",
    "fastify-swagger": "^5.2.0",
    "nestjs-cls": "^5.4.2",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pino-http": "^10.4.0",
    "redis": "^4.7.0",
    "reflect-metadata": "0.2",
    "typia": "^8.0.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@nestia/benchmark": "^0.4.3",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.12",
    "@types/node": "^22.13.13",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fastify": "^5.2.2",
    "nestia": "^7.0.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "prisma-markdown": "^1.0.9",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "^5.8.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "vitest-mock-extended": "^3.0.1"
  }
}
